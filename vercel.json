{
  "version": 2,
  "name": "voting-results",
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "api/data.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/data",
      "dest": "/api/data.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
